name: "defos"

platforms:
  x86_64-osx:
    context:
        frameworks: ["AppKit"]
        
  x86-osx:
    context:
        frameworks: ["AppKit"]

  x86_64-linux:
    context:      
        frameworks: ["X11", "Xrender"]
        libs: ["Xrender", "Xfixes"]
        
 x86_64-win32:
     context:
         flags: [“-Wno-c++11-narrowing”]
         
 x86-win32:
     context:
         flags: [“-Wno-c++11-narrowing”]
